var nowPageNum = 1; // 첫 페이지 번호
var totalPageNum = 10; // 전체 페이지 수 !!!!!! 여기만 수정하기
var currentPage = nowPageNum;
function goNextPage() {
    if (currentPage < totalPageNum) {
        opener.PageMove(currentPage);
        console.log(`${currentPage} 페이지를 수강완료했습니다.`);
        currentPage += 1;
        setTimeout(function () {
            goNextPage();
        }, 100);
    } else {
        opener.PageMove(currentPage);
        console.log('강의 수강이 완료되었습니다!');
        setTimeout(function () {
            window.close();
        }, 200);
    }
}
setTimeout(function () {
    goNextPage();
}, 100);
